{
  "name": "element-util",
  "version": "1.6.5",
  "description": "The utility library that provides convenience for using HTML Element.",
  "main": "dist/element-util.js",
  "types": "element-util.d.ts",
  "directories": {
    "dist": "dist",
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "build": "webpack --hide-modules",
    "watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/archco/element-util.git"
  },
  "keywords": [
    "DOM",
    "library"
  ],
  "author": "archco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/archco/element-util/issues"
  },
  "homepage": "https://github.com/archco/element-util#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2",
    "webpack-notifier": "^1.8.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/element-util.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
