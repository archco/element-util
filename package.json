{
  "name": "element-util",
  "version": "1.6.9",
  "description": "The utility library that provides convenience for using HTML Element.",
  "main": "dist/element-util.js",
  "types": "element-util.d.ts",
  "directories": {
    "dist": "dist",
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/archco/element-util.git"
  },
  "keywords": [
    "DOM",
    "library"
  ],
  "author": "archco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/archco/element-util/issues"
  },
  "homepage": "https://github.com/archco/element-util#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-jest": "^26.6.3",
    "browserslist": "^4.16.6",
    "eslint": "^7.26.0",
    "jest": "^26.6.3",
    "source-map-loader": "^2.0.1",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.3",
    "webpack-notifier": "^1.13.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/element-util.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
